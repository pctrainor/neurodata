{
  "name": "neurodata-hub",
  "version": "0.1.0",
  "private": true,
  "description": "NeuroData Hub - Open neuroscience data marketplace aggregating OpenNeuro, Allen Brain Atlas, and HCP",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npx tsx scripts/fetch-trending-videos.ts || true",
