    "agent:enrich": "npx ts-node --esm agents/data-enricher.ts",
    "agent:quality": "npx ts-node --esm agents/quality-checker.ts",
    "sync:openneuro": "npx ts-node scripts/sync-openneuro.ts",
    "sync:allen": "npx ts-node scripts/sync-allen.ts",
    "sync:hcp": "npx ts-node scripts/sync-hcp.ts",
    "sync:upload": "npx ts-node scripts/upload-to-supabase.ts",
    "sync:all": "npx ts-node scripts/sync-all.ts"
