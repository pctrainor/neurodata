# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Google Gemini API (for AI agents and data enrichment)
GEMINI_API_KEY=your-gemini-api-key

# Site Configuration
NEXT_PUBLIC_SITE_NAME=Data Platform
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Development Mode (set to 'true' to bypass auth in development)
NEXT_PUBLIC_DEV_MODE=false
DEV_MODE_USER_ID=00000000-0000-0000-0000-000000000001

# Optional: External Data Source APIs
# Add your data source API keys here
# DATA_GOV_API_KEY=your-data-gov-key
# OTHER_API_KEY=your-other-api-key

# YouTube Data API key (for video discovery during onboarding)
# Get this from Google Cloud Console: https://console.cloud.google.com/apis/library/youtube.googleapis.com
YOUTUBE_API_KEY=
